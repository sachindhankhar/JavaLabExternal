package q2_bookDb;

import java.util.ArrayList;

public class MainClass {
	public static void main(String []args) {
		ArrayList<Book> al=new ArrayList<Book>();
		al.add(new Book("java","sanchit","tmh",100.0));
		al.add(new Book("dbms","sanchit","tmh",200.5));
		al.add(new Book("daa","sanchit","tmh",450.0));
		al.add(new Book("ml","sanchit","tmh",399.0));
		
		System.out.println(al);
	}
}
